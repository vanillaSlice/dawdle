{
  "extends": [
    "config:base",
    ":prHourlyLimitNone"
  ],
  "pip_requirements": {
    "fileMatch": [
      "^requirements.*\\.txt$"
    ]
  },
  "docker": {
    "fileMatch": [
      ".*Dockerfile$",
      "docker-compose.yml"
    ]
  },
  "automerge": true,
  "major": {
    "automerge": false
  },
  "rebaseStalePrs": true
}
